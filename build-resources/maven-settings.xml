<settings>
    <mirrors>
        <mirror>
            <id>Internal-maven-repo</id>
            <name>VMware Nexus Repo</name>
            <url>http://build-squid.eng.vmware.com/nexus/content/groups/repo
            </url>
            <mirrorOf>external:*</mirrorOf>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>gobuild</id>
            <properties>
                <keystoreLocation>../build-resources/VMware_Master_Certificate.vmokeystore</keystoreLocation>
                <keystoreFileLocation>../build-resources/VMware_Master_Certificate.vmokeystore</keystoreFileLocation>
                <keystorePassword>vmware01</keystorePassword>
                <installation.mode>always</installation.mode>
            </properties>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>gobuild</activeProfile>
    </activeProfiles>
</settings>